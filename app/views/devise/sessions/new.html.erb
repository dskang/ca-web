<% content_for(:form_title, "Sign In To Your Campus Anonymous Account") %>
<div class="row text-center">
  <div class="col-md-12">
    <%= bootstrap_form_for(resource, style: :horizontal, :as => resource_name, :url => session_path(resource_name)) do |f| %>
      <%= f.email_field :email, :autofocus => true %>
      <%= f.password_field :password %>
      <%= f.check_box :remember_me %>
      <%= f.submit "Sign In", class: "go-btn btn btn-success btn-xlg btn-block" %>
    <% end %>
    <%= render "devise/shared/links" %>
  </div>
</div>
