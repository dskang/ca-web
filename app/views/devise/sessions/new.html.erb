<h2>Log in</h2>

<%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= f.email_field :email, hide_label: true, class: "input-lg", placeholder: "Email", autofocus: true %>
  <%= f.password_field :password, hide_label: true, class: "input-lg", placeholder: "Password" %>
  <%= f.submit "Sign In", class: "btn btn-success btn-lg btn-block" %>
<% end %>

<%= render "devise/shared/links" %>
